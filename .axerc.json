{
  "locale": "it",
  "reporter": "v2",
  "outputDir": "./axe-reports",
  "tags": ["wcag2a", "wcag2aa", "wcag21a", "wcag21aa", "best-practice"],
  "rules": {
    "color-contrast": {
      "enabled": true
    },
    "image-alt": {
      "enabled": true
    },
    "label": {
      "enabled": true
    },
    "landmark-one-main": {
      "enabled": true
    },
    "page-has-heading-one": {
      "enabled": true
    },
    "region": {
      "enabled": true
    }
  },
  "crawl": false,
  "exclude": [],
  "timeout": 120000
}
